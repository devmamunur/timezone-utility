  },
  "homepage": "https://github.com/devmamunur/timezone-utility#readme",
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "rollup": "^4.24.0",
    "tslib": "^2.8.0",
    "typescript": "^5.0.0"
  }
